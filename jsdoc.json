{
    "source": {
        "include": ["./server/src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/category"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./readme.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/better-docs"
    },
    "tags": {
        "allowUnknowTags": true
    }
}